<shape xmlns:android="http://schemas.android.com/apk/res/android">
	<stroke
		android:width="8dp"
		android:dashGap="2dp"
		android:dashWidth="8dp"
		android:color="#6000FF00" />
	<gradient
		android:startColor="#FFFF6600"
		android:endColor="#80FF8C00"
		android:angle="0" />
	<padding
		android:left="12dp"
		android:top="12dp"
		android:right="12dp"
		android:bottom="12dp" />
	<corners android:radius="24dp" />
</shape>
