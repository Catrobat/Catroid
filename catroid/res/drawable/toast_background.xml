	<stroke
		android:width="1dp"
		android:dashGap="0dp"
		android:dashWidth="1dp"
		android:color="#000000" />
	<gradient
		android:startColor="#ff8f18"
		android:endColor="#f8af3c"
		android:angle="0" />
	<padding
		android:left="12dp"
		android:top="12dp"
		android:right="12dp"
		android:bottom="12dp" />
	<corners android:radius="24dp" />
</shape>
