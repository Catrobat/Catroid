<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="20%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/key_height"
    >

    <Row>
        <Key android:codes="8" android:keyLabel="1" android:keyEdgeFlags="left"/>
        <Key android:codes="9" android:keyLabel="2"/>
        <Key android:codes="10" android:keyLabel="3"/>
        <Key android:codes="81" android:keyLabel="+"/>
        <Key android:codes="69" android:keyLabel="-" android:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key android:codes="11" android:keyLabel="4" android:keyEdgeFlags="left"/>
        <Key android:codes="12" android:keyLabel="5"/>
        <Key android:codes="13" android:keyLabel="6"/>
        <Key android:codes="17" android:keyLabel="*"/>
        <Key android:codes="76" android:keyLabel="/" android:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key android:codes="14" android:keyLabel="7" android:keyEdgeFlags="left"/>
        <Key android:codes="15" android:keyLabel="8"/>
        <Key android:codes="16" android:keyLabel="9"/>
        <Key android:codes="67" android:keyIcon="@drawable/sym_keyboard_delete" android:isRepeatable="true"/>
        <Key android:codes="26" android:keyLabel="^" android:keyEdgeFlags="right"/>
    </Row> 
    
    <Row  android:rowEdgeFlags="bottom">
        <Key android:codes="7" android:keyLabel="0" />
        <Key android:codes="55" android:keyLabel="," android:keyWidth="20%p" />
        <Key android:codes="62" android:keyIcon="@drawable/sym_keyboard_space" android:keyWidth="40%p" android:isRepeatable="true"/> 
        <Key android:codes="60" android:keyLabel="1/3" android:keyWidth="20%p" android:isModifier="true" android:keyEdgeFlags="right" />
    </Row>
</Keyboard>