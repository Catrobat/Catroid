<PreferenceScreen
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:title="@string/preference_title_extension">

    <PreferenceScreen
        app:iconSpaceReserved="false"
        android:key="settings_ai_screen"
        android:title="@string/preference_title_ai"
        android:summary="@string/preference_description_ai" />

    <PreferenceScreen
        app:iconSpaceReserved="false"
        android:key="setting_nxt_screen"
        android:title="@string/preference_title_enable_mindstorms_nxt_bricks"
        android:summary="@string/preference_description_mindstorms_nxt_bricks" />

    <PreferenceScreen
        app:iconSpaceReserved="false"
        android:key="setting_ev3_screen"
        android:title="@string/preference_title_enable_mindstorms_ev3_bricks"
        android:summary="@string/preference_description_mindstorms_ev3_bricks" />

    <PreferenceScreen
        app:iconSpaceReserved="false"
        android:key="settings_drone_screen"
        android:title="@string/preference_title_enable_quadcopter_bricks"
        android:summary="@string/preference_description_quadcopter_bricks" />

    <PreferenceScreen
        app:iconSpaceReserved="false"
        android:key="settings_raspberry_screen"
        android:title="@string/preference_title_enable_raspi_bricks"
        android:summary="@string/preference_description_raspi_bricks" />

    <SwitchPreference
        app:iconSpaceReserved="false"
        android:defaultValue="false"
        android:key="setting_arduino_bricks"
        android:title="@string/preference_title_enable_arduino_bricks"
        android:summary="@string/preference_description_arduino_bricks" />

    <SwitchPreference
        app:iconSpaceReserved="false"
        android:defaultValue="false"
        android:key="setting_cast_globally_enabled"
        android:title="@string/preference_title_cast_feature_globally_enabled"
        android:summary="@string/preference_description_cast_feature_globally_enabled" />

    <SwitchPreference
        app:iconSpaceReserved="false"
        android:defaultValue="@string/FEATURE_EMBROIDERY_PREFERENCES_ENABLED"
        android:key="setting_embroidery_bricks"
        android:title="@string/preference_title_enable_embroidery_bricks"
        android:summary="@string/preference_description_embroidery_bricks" />

    <SwitchPreference
        app:iconSpaceReserved="false"
        android:defaultValue="false"
        android:key="setting_nfc_bricks"
        android:title="@string/preference_title_enable_nfc_bricks"
        android:summary="@string/preference_description_nfc_bricks" />

    <SwitchPreference
        app:iconSpaceReserved="false"
        android:defaultValue="false"
        android:key="setting_parrot_jumping_sumo_bricks"
        android:title="@string/preference_title_enable_jumpingsumo_bricks"
        android:summary="@string/preference_description_jumpingsumo_bricks" />

    <SwitchPreference
        app:iconSpaceReserved="false"
        android:defaultValue="@string/FEATURE_PHIRO_PREFERENCES_ENABLED"
        android:key="setting_enable_phiro_bricks"
        android:title="@string/preference_title_enable_phiro_bricks"
        android:summary="@string/preference_description_phiro_bricks" />

    <SwitchPreference
        app:iconSpaceReserved="false"
        android:defaultValue="false"
        android:key="setting_multiplayer_variables_enabled"
        android:title="@string/preference_title_multiplayer_variables_enabled"
        android:summary="@string/preference_description_multiplayer_variables_enabled" />

    <SwitchPreference
        app:iconSpaceReserved="false"
        android:defaultValue="@string/DEBUG_MODE"
        android:key="setting_test_bricks"
        android:title="@string/preference_title_enable_test_bricks"
        android:summary="@string/preference_description_test_bricks" />

</PreferenceScreen>